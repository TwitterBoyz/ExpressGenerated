<header>
  <h1>{{username}}'s profile</h1>
</header>
  <a href="/"><h3>Back To Home</h3></a>
<section>
  <div id="tweets">
    {{#each uniqueMessages}}
    <div class="username">
      {{username}} wrote: "{{message}}" at {{time}}
    </div>
    {{/each}}
  </div>
  <div class="send message">
    <form class="" action="/directMessage" method="post">
    <input type="text" name="message" id = "message" value="">
    <input type="hidden" name="username" id = "username" value="{{username}}">
    <input type="submit" name="name" value="Send {{username}} a message">
      </form>
  </div>
</section>
